    "@mui/material": "^5.11.10",
    "@reduxjs/toolkit": "^1.9.3",
    "chart.js": "^4.2.1",
    "csv": "^6.2.8",
    "date-fns": "^2.29.3",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^6.0.4",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
